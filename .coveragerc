[run]
source = doublehelix
branch = true
omit =
    */tests/*
    */__pycache__/*
    */alembic/*
    */.venv/*

[report]
fail_under = 80
show_missing = true
skip_covered = false
exclude_lines =
    pragma: no cover
    raise NotImplementedError
    if TYPE_CHECKING:
    if __name__ == .__main__.:
    @abstractmethod
    @abc.abstractmethod
    def __repr__
    def __str__

[html]
directory = htmlcov

[xml]
output = coverage.xml
